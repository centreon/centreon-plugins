{
  "collect": {
    "snmp": [
      {
	"name": "cpu",
        "oid": "1.3.6.1.2.1.25.3.3.1.2",
	"query": "Walk",	    // par d√©faut walk
      }
    ]
  },
  "compute": {
    "metrics": [
      {
        "name": "core.cpu.usage.percent",
        "value": "{cpu}",
	"uom": "%",     // optional
	"min": 0,       // optional
	"max": 100      // optional
      }
    ],
    "aggregations": [
      {
        "name": "avg.cpu.usage.percent",
	"value": "Average({cpu})",	  // si agregation, pas de prefix
	"uom": "%",     // optional
	"min": 0,       // optional
	"max": 100      // optional
      }
    ]
  }
}
