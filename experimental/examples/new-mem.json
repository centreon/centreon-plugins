{
  "collect": {
    "snmp": [
      {
	"name": "total",
	"oid": "1.3.6.1.4.1.2021.4.6.0",
	"query": "Get",	    // par défaut walk
      },
      {
	"name": "free",
	"oid": "1.3.6.1.4.1.2021.4.11.0",
	"query": "Get",	    // par défaut walk
      }
    ]
  },
  "compute": {
    "metrics": [
      {
        "name": "core.mem.usage.percent",
        "value": "100 * (1 - {free}/{cpu})",
	"uom": "%",     // optional
	"min": 0,       // optional
	"max": 100      // optional
      },
      {
        "name": "avg.cpu.usage.percent",
	"value": "Average({cpu})",	  // si agregation, pas de prefix
	"uom": "%",     // optional
	"min": 0,       // optional
	"max": 100      // optional
      }
    ]
  }
}

